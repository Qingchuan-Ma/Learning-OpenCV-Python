{
  "title":"学习 OpenCV (Python)",
  "author":"Qingchuan-Ma",
  "language":"zh-hans",
  "links": {
    "sidebar":{
      "GitHub":"https://github.com/Qingchuan-Ma/Learning-OpenCV-Python"
    }
  },
  "plugins": [
    "-livereload",
    "image-captions",
    "github",
    "page-treeview",
    "editlink",
    "prism",
    "-highlight",
    "splitter",
    "sitemap-general",
    "expandable-chapters-small",
    "anchor-navigation-ex"
  ],
  "pluginsConfig": {
    "image-captions": {
      "attributes": {
        "width": "600"
      },
      "caption": "图 _PAGE_LEVEL_._PAGE_IMAGE_NUMBER_ - _CAPTION_"
    },
    "github": {
      "url": "https://github.com/Qingchuan-Ma/Learning-OpenCV-Python"
    },
    "editlink": {
      "base": "https://github.com/Qingchuan-Ma/Learning-OpenCV-Python/edit/master",
      "label": "编辑本页"
    },
    "page-treeview": {
      "copyright": "Copyright &#169; qingchuan-ma",
      "minHeaderCount": "2",
      "minHeaderDeep": "2"
    },
    "prism": {
    "css": [
      "prismjs/themes/prism-coy.css"
    ]
    },
    "sitemap-general": {
      "prefix": "http://gitbook.qingchuan-ma.com"
    },
    "anchor-navigation-ex": {
      "isRewritePageTitle": false,
      "tocLevel1Icon": "fa fa-hand-o-right",
      "tocLevel2Icon": "fa fa-hand-o-right",
      "tocLevel3Icon": "fa fa-hand-o-right"
    }
  }
}
